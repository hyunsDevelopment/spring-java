<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kiwoom.app.sample.mapper.SampleMapper">

	<select id="getMember" resultType="hmap">
		SELECT * FROM USR_USERMNG
		WHERE USER_ID = #{userId}
	</select>

	<select id="getMemberList" resultType="hmap">
		SELECT * FROM USR_USERMNG
		WHERE USER_STCD = #{userStcd}
	</select>
	
</mapper>